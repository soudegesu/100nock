    "def cipher(text):\n",
    "    arr = []\n",
    "    for c in text:\n",
    "        arr.append(chr(219 - ord(c)) if c.islower() else c)\n",
    "    return \"\".join(arr)\n",
    "             \n",
    "print(cipher('AbcDeFg'))"
